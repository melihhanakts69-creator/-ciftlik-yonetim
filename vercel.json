{
  "installCommand": "rm -rf node_modules && npm install",
  "buildCommand": "CI=false REACT_APP_API_URL=https://ciftlik-yonetim.onrender.com/api npm run build",
  "env": {
    "REACT_APP_API_URL": "https://ciftlik-yonetim.onrender.com/api"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}